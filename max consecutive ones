potd
#include <iostream>
#include<algorithm>
using namespace std;
int main() {
    // Write C++ code here
   int n,i,c=0,maxi=0;
   cin>>n;
   string s;
   cin>>s;
   for(i=0;i<s.length();i++){
       if(s[i]=='1'){
           c++;
       }
       else{
           maxi=max(maxi,c);
           c=0;
       }
   }
   cout<<maxi;
   
    return 0;
}
output
5
11101
3
